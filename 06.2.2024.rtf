<!DOCTYPE html>\par
<html lang="en">\par
<head>\par
  <meta charset="UTF-8">\par
  <meta name="viewport" content="width=device-width, initial-scale=1.0">\par
  <title>Kalendarz</title>\par
  <style>\par
    table \{\par
      border-collapse: collapse;\par
      width: 100%;\par
    \}\par
    th, td \{\par
      border: 1px solid #ddd;\par
      padding: 8px;\par
      text-align: center;\par
    \}\par
    th \{\par
      background-color: #f2f2f2;\par
    \}\par
  </style>\par
</head>\par
<body>\par
\par
<h2>Kalendarz</h2>\par
\par
<table>\par
  <thead>\par
    <tr>\par
      <th>Niedziela</th>\par
      <th>Poniedzia\'b3ek</th>\par
      <th>Wtorek</th>\par
      <th>\'8croda</th>\par
      <th>Czwartek</th>\par
      <th>Pi\'b9tek</th>\par
      <th>Sobota</th>\par
    </tr>\par
  </thead>\par
  <tbody id="calendar-body">\par
    <!-- W tym miejscu zostanie wstawiona tabela kalendarza -->\par
  </tbody>\par
</table>\par
\par
<script>\par
  document.addEventListener('DOMContentLoaded', function() \{\par
    // Pobierz aktualn\'b9 dat\'ea\par
    var currentDate = new Date();\par
\par
    // Ustaw dat\'ea na pierwszy dzie\'f1 aktualnego miesi\'b9ca\par
    currentDate.setDate(1);\par
\par
    // Pobierz dzie\'f1 tygodnia pierwszego dnia miesi\'b9ca\par
    var firstDay = currentDate.getDay();\par
\par
    // Pobierz liczb\'ea dni w aktualnym miesi\'b9cu\par
    var lastDay = new Date(currentDate.getFullYear(), currentDate.getMonth() + 1, 0).getDate();\par
\par
    // Pobierz element tbody\par
    var calendarBody = document.getElementById('calendar-body');\par
\par
    // Inicjalizuj zmienn\'b9 do przechowywania kom\f1\lang1033\'f3rek tabeli\par
    var cells = '';\par
\par
    // Dodaj puste kom\'f3rki przed pierwszym dniem miesi\f0\'b9ca\par
    for (var i = 0; i < firstDay; i++) \{\par
      cells += '<td></td>';\par
    \}\par
\par
    // Dodaj dni miesi\'b9ca\par
    for (var day = 1; day <= lastDay; day++) \{\par
      cells += '<td>' + day + '</td>';\par
      if ((firstDay + day - 1) % 7 === 0) \{\par
        cells += '</tr><tr>';\par
      \}\par
    \}\par
\par
    // Uzupe\'b3nij puste kom\f1\'f3rki po ostatnim dniu miesi\f0\'b9ca\par
    var remainingCells = 7 - (lastDay + firstDay) % 7;\par
    for (var j = 0; j < remainingCells; j++) \{\par
      cells += '<td></td>';\par
    \}\par
\par
    // Umie\'9c\'e6 kom\f1\'f3rki w tabeli\par
    calendarBody.innerHTML = '<tr>' + cells + '</tr>';\par
  \});\par
</script>\par
\par
</body>\par
</html>\par
